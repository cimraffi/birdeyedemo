<?xml version="1.0" encoding="utf-8"?>

<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 设置填充颜色  -->
    <solid android:color="@color/dialog_button_pressed" />

    <!-- 设置几何图形四个角的弧度 -->
    <corners android:topLeftRadius="0dp" android:bottomLeftRadius="8dp"  
        android:topRightRadius="0dp" android:bottomRightRadius="8dp" />

    <!-- 设置几何形状绘制边框 -->
    <stroke
        android:width="0dp"
        android:color="@color/dialog_button_pressed" />

</shape>
